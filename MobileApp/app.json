{
  "expo": {
    "name": "Quản Lý Nhà Thuốc",
    "slug": "pharmacy-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pharmacy.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pharmacy.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pharmacyapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker"
    ],
    "scheme": "pharmacyapp",
    "sdkVersion": "54.0.0",
    "extra": {
      "eas": {
        "projectId": "8835182a-7467-41b9-8369-6a82594ad730"
      }
    }
  }
}
